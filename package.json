{
  "name": "gitbook-tester",
  "version": "1.0.0",
  "description": "Tester for gitbook plugins",
  "main": "index.js",
  "scripts": {
    "lint": "jshint index.js spec/**.js lib/**.js",
    "pretest": "npm run lint",
    "test": "istanbul cover -x **/spec/** jasmine-node -- spec --verbose --captureExceptions",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "gitbook",
    "plugin",
    "tester"
  ],
  "author": "Tomas Dvorak <todvora@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "cheerio": "0.19.0",
    "child-process-promise": "~1.0.2",
    "findit": "2.0.0",
    "q": "~1.2.0",
    "temp": "^0.8.3",
    "gitbook-cli": "1.0.0"
  },
  "devDependencies": {
    "jasmine-node": ">=1.14.5",
    "istanbul": ">=0.3.13",
    "jshint": ">=2.6.3",
    "coveralls": ">=2.11.2"
  }
}
